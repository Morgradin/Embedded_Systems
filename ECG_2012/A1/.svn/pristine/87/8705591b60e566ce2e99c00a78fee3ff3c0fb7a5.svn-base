#include <stdio.h>
#include <stdlib.h>
#include "sensor.h"

void testLow(int Data[], int FilteredData[]){

	int i = 0;
	for (i=0;i<50;i++){
			FilteredData[i] = lowPass(Data, FilteredData, i);
	}

	for (i=0;i<50;i++){
		printf("Before filter: %d. After Filter: %d\n", Data[i], FilteredData[i]);
	}
	return FilteredData;
}

